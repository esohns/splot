/* config.h.cmake */

/* Name of package */
#cmakedefine PACKAGE "@PACKAGE@"

/* Define to the address where bug reports for this package should be sent. */
#cmakedefine PACKAGE_BUGREPORT

/* Define to the full name of this package. */
#define SPLOT_PACKAGE_NAME "@PACKAGE@"

/* Define to the full name and version of this package. */
#define SPLOT_PACKAGE_STRING "@PACKAGE@ @VERSION@"

/* Define to the one symbol short name of this package. */
#cmakedefine PACKAGE_TARNAME

/* Define to the home page for this package. */
#cmakedefine PACKAGE_URL

/* Define to the version of this package. */
#define SPLOT_PACKAGE_VERSION "@VERSION@"

/* Version number of package */
#define SPLOT_VERSION_MAJOR @VERSION_MAJOR@
#define SPLOT_VERSION_MINOR @VERSION_MINOR@
#define SPLOT_VERSION_MICRO @VERSION_MICRO@
#define SPLOT_VERSION_DEVEL "@VERSION_DEVEL@"
#cmakedefine VERSION "@VERSION@"

// options
#cmakedefine TRACING
#cmakedefine VALGRIND_SUPPORT
#cmakedefine WINXP_SUPPORT
